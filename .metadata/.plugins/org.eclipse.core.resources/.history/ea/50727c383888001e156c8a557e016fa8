    <%
    
    public void EvenCalc(n){
        String sign;
    	if(n % 2 == 0){
    		sign="짝수";
    	}else if(n % 2 == 1){
    		sign="홀수";
    	}else{
    		sign="null";
    	}
        out.println("결과는 " + sign + "입니다.");

    }
    
    %>
    